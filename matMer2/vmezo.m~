clear all ; 
clf ;

K = sqrt(28/3) ;
f = @(t, y)  6*t .* y.^2 ;

tb = -K:0.2:2 ;
yb = 0:0.2:3 ;
[T,Y] = meshgrid(tb, yb) ;
dY = f(T, Y) ;
dT = ones(size(dY)) ;
quiver(T,Y, dT, dY) ;

% t = -3/2, y = 0 => C
C = 1 + 0.5 * 1.5^5 ;

mo = @(x) (-0.75*x .^ 4 + C) .^ (1/3) - 1 ;
xx = linspace(-1.5,1.5) ;
hold on ;
plot(xx, mo(xx)) ;






