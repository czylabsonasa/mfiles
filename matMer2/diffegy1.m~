        0>0;
          1;
       1----1;

 
clear all ;
clf ;

feladatok
F = lista{1} ;

eval(F.init) ;
egyenlet = F.symf ;
f = F.numf ;
xx = F.xx ;
tb = F.tb ;
yb = F.yb ;
IVP = F.IVP ;

% altal√°nos
gensol = dsolve(egyenlet)
[T,Y] = meshgrid(tb, yb) ;
dY = f(T,Y) ;
dT = ones( size(dY) ) ;
quiver(T,Y,dT,dY) ;
axis([tb(1) tb(end) yb(1) yb(end)]) ;

hold on ;

for v=IVP
  a = v(1) ;
  b = v(2) ;
  aktsol = matlabFunction(dsolve(equ, y(a)==b)) ;
  plot(xx, aktsol(xx)) ;
end
legend('quiver', 'kp1', 'kp2', 'kp3') ;
legend('location','northeastoutside') ;

save 'rajz.pdf'
