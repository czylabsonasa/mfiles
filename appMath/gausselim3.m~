function [L,U]=gausselim3(A)
% 
% LU - also print the L 
% 
  U = A ; % copy - not a reference
  s = size() ;
  r = s(1) ;
  c = s(2) ;
  if not(r == c ) 
    fprintf('A must be square') ;
    return ;
  end
  L = eye(r,r) ;

  fprintf('\nphase 0:\n') ;
  printout([L,A]) ;
  for i=1:r-1
    fprintf('\nphase %d:\n', i) ;
    if abs(A(i,i))==0
      if any(abs(A(i+1:end,i))>0)
        fprintf('no way w/o swap...exiting...\n');
        return ;
      else
        fprintf('no need to eliminate\n');
        paktc() ;
        continue ;
      end
    end
    p = 1.0/A(i,i) ;
    for j=(i+1):r
      fprintf('  sub-phase %d:\n', j) ;
      lji = p*A(j,i) ;
      L(j,i)=lji ;
      if abs(lji)>0
        A(j,:) = A(j,:) - lji*A(i,:) ;
        fprintf('    subtract %s times the %d. row from the %d. row\n\n', strtrim(rats(lji)), i, j);
        printout([L,A]) ;
      else
        fprintf('    nothing to eliminate\n\n');
      end
      paktc() ;
    end
  end
  fprintf('\n') ;
  % here L
end
